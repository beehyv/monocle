{
  "type": "retrieval",
  "attributes": [
    [
      {
        "_comment": "vector store name and type",
        "attribute": "name",
        "accessor": "lambda instance,args: resolve_from_alias(instance.__dict__, ['document_store', '_document_store']).__class__.__name__"
      },
      {
        "attribute": "type",
        "accessor": "lambda instance,args: 'vectorstore.'+resolve_from_alias(instance.__dict__, ['document_store', '_document_store']).__class__.__name__"
      },
      {
        "attribute": "deployment",
        "accessor": "lambda instance,args: get_vectorstore_deployment(resolve_from_alias(instance.__dict__, ['document_store', '_document_store']).__dict__)"
      }
    ],
    [
      {
        "_comment": "embedding model name and type",
        "attribute": "name",
        "accessor": "lambda instance,args: get_embedding_model()"
      },
      {
        "attribute": "type",
        "accessor": "lambda instance ,args: 'model.embedding.'+get_embedding_model()"
      }
    ]
  ]
}
